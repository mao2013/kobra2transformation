package Data::Kobra2::SUM::Structure::Elements

context Element inv elementInv:
self.oclIsKindOf(StructuralElement) or
self.oclIsKindOf(StructuralBehavioralElement) or
self.oclIsKindOf(ConstraintElement) or
self.oclIsKindOf(BehavioralElement)

context StructuralElement inv structuralInv:
self.oclIsKindOf(TypeElement) or
self.oclIsKindOf(InstanceElement)

endpackage


package Data::Kobra2::SUM::Structure::Types

context DataType inv dataTypeInv:
self.isAbstract

endpackage